                    {x: 145, y: 470, width: 200, height: 10},
                    {x: 370, y: 400, width: 180, height: 10},
                    {x: 515, y: 310, width: 150, height: 10}
                ],
                movingPlatforms: [
                    {x: 230, y: 300, width: 50, height: 12, startX: 230, endX: 300, speed: 5, direction: 1},
                    {x: 520, y: 180, width: 50, height: 12, startX: 420, endX: 580, speed: 4.5, direction: -1}
                ],
                goal: {x: 745, y: 150, width: 40, height: 40},
                checkpointPos: {x: 320, y: 340}
            }
        ];

        let currentLevel = levels[0];

        function resetPlayer() {
            player.x = checkpoint.x;
            player.y = checkpoint.y;
            player.velocityX = 0;
            player.velocityY = 0;
            player.onGround = false;
        }

        function loadLevel(levelNum) {
            gameState.currentLevel = levelNum;
            currentLevel = levels[levelNum - 1];
            checkpoint = {x: 50, y: 500, active: false};
            resetPlayer();
            
            // Update UI
            document.getElementById('currentLevel').textContent = levelNum;
            const diffSpan = document.getElementById('difficulty');
            diffSpan.textContent = currentLevel.difficulty.charAt(0).toUpperCase() + currentLevel.difficulty.slice(1);
            diffSpan.className = 'difficulty ' + currentLevel.difficulty;
            document.getElementById('checkpoints').textContent = gameState.checkpoints;
        }

        function restartLevel() {
            document.getElementById('gameOver').style.display = 'none';
            gameState.gameRunning = true;
            resetPlayer();
        }

        function restartGame() {
            gameState.currentLevel = 1;
            gameState.deaths = 0;
            gameState.checkpoints = 0;
            gameState.gameRunning = true;
            document.getElementById('gameOver').style.display = 'none';
            document.getElementById('victory').style.display = 'none';
            document.getElementById('deaths').textContent = '0';
            loadLevel(1);
        }

        function checkCollision(rect1, rect2) {
            return rect1.x < rect2.x + rect2.width &&
                   rect1.x + rect1.width > rect2.x &&
                   rect1.y < rect2.y + rect2.height &&
                   rect1.y + rect1.height > rect2.y;
        }

        function update() {
            if (!gameState.gameRunning) return;

            // Handle input
            if (keys['arrowleft'] || keys['a']) {
                player.velocityX = -player.speed;
            } else if (keys['arrowright'] || keys['d']) {
                player.velocityX = player.speed;
            } else {
                player.velocityX = 0;
            }

            // Apply gravity
            player.velocityY += player.gravity;

            // Update position
            player.x += player.velocityX;
            player.y += player.velocityY;

            // Check ground collision
            player.onGround = false;

            // Static platforms
            currentLevel.platforms.forEach(platform => {
                if (checkCollision(player, platform)) {
                    if (player.velocityY > 0) {
                        player.y = platform.y - player.height;
                        player.velocityY = 0;
                        player.onGround = true;
                    }
                }
            });

            // Moving platforms
            currentLevel.movingPlatforms.forEach(platform => {
                // Update platform position
                platform.x += platform.speed * platform.direction;
                if (platform.x >= platform.endX || platform.x <= platform.startX) {
                    platform.direction *= -1;
                }

                if (checkCollision(player, platform)) {
                    if (player.velocityY > 0 && player.y + player.height - player.velocityY <= platform.y) {
                        player.y = platform.y - player.height;
                        player.velocityY = 0;
                        player.onGround = true;
                        player.x += platform.speed * platform.direction;
                
