            },
            // Level 10 - Final Challenge
            {
                difficulty: 'extreme',
                platforms: [
                    {x: 0, y: 550, width: 70, height: 50, type: 'normal'},
                    {x: 100, y: 490, width: 45, height: 12, type: 'normal'},
                    {x: 170, y: 440, width: 40, height: 12, type: 'normal'},
                    {x: 320, y: 370, width: 50, height: 12, type: 'normal'},
                    {x: 470, y: 280, width: 45, height: 12, type: 'normal'},
